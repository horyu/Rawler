      xmlns:YahooAPI="clr-namespace:RawlerYahooAPI;assembly=RawlerYahooAPI"
      >
    <SetWorkFolder SpecialFolder="MyDocuments" Folder="ORF2013/しんり"></SetWorkFolder>
    <SetTempVar Key="YahooApiId" Value="dj0zaiZpPXIyUk1qeFVJd1YzcyZzPWNvbnN1bWVyc2VjcmV0Jng9MGY-"/>

    <Data FileName="Keyphrase_風立ちぬ_good.tsv">
        <TsvReadLines FileName="reviw_風立ちぬ_good.tsv">
