<UserControl x:Class="Rawler.RawlerLib.WPF.ViewTextControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl Name="tabControl1" TabStripPlacement="Bottom">
            <TabItem Header="Text" Name="tabItem1">
                <Grid>
                    <TextBox Name="textBox1" />
                </Grid>
            </TabItem>
            <TabItem Header="ブラウザ" Name="tabItem2">
                <Grid>
                    <WebBrowser Name="webBrowser1" />
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
