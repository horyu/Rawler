        <TsvReadLines FileName="ももクロCommentData.tsv">
            <GetTsvValue ColumnName="Content">
                <m:MecabPreprocessing>
                    <m:Mecab ViewType="表層形" UseHinshi="名詞,形容詞,動詞">
                        <m:Mecab.Query>
                            <QueryDistinct></QueryDistinct>
                        </m:Mecab.Query>
                        <Equal Result="True">
                            <Equal.EqualDataTree>
                                <Iterator>
                                    <Iterator.SourceTree>
                                        <FileReadLines FileName="ももくろ共通部分.txt">
                                            <IteratorSourceAddText></IteratorSourceAddText>
                                        </FileReadLines>
                                    </Iterator.SourceTree>
                                </Iterator>
                            </Equal.EqualDataTree>
                            <DataWrite Attribute="Text"></DataWrite>
                        </Equal>
                    </m:Mecab>
                </m:MecabPreprocessing>
                <NextDataRow IgnoreDataNull="True"></NextDataRow>
            </GetTsvValue>
        </TsvReadLines>
    </FileSave>

